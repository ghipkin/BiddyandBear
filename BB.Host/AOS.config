  <PivotalClosureConfiguration>
    <Pivotal_Closure_Mappings>
      <Pivotal_Closure>
        <!-- NOTES - Reprocessing a application because the details have changed -->
        <Application_Status_Code>AR</Application_Status_Code>
        <CustomerClosureCodePopulated>false</CustomerClosureCodePopulated>
        <ManagerClosureCodePopulated>false</ManagerClosureCodePopulated>
        <IncludedClosureCodes />
        <PivotalClosureReason>Closed - Changed Mind</PivotalClosureReason>
      </Pivotal_Closure>
      <Pivotal_Closure>
        <!-- NOTES - Manager closes in Prospects as Changed Mind -->
        <Application_Status_Code>CM</Application_Status_Code>
        <CustomerClosureCodePopulated>true</CustomerClosureCodePopulated>
        <ManagerClosureCodePopulated>false</ManagerClosureCodePopulated>
        <IncludedClosureCodes />
        <PivotalClosureReason>Closed - Changed Mind</PivotalClosureReason>
      </Pivotal_Closure>
      <Pivotal_Closure>
        <!-- NOTES - Manager closes in Prospects as Declined -->
        <Application_Status_Code>CM</Application_Status_Code>
        <CustomerClosureCodePopulated>false</CustomerClosureCodePopulated>
        <ManagerClosureCodePopulated>true</ManagerClosureCodePopulated>
        <IncludedClosureCodes />
        <PivotalClosureReason>Closed - Field Declined</PivotalClosureReason>
      </Pivotal_Closure>
      <Pivotal_Closure>
        <!-- NOTES - Manager Declines a Level 2 Authorisation request -->
        <Application_Status_Code>DM</Application_Status_Code>
        <CustomerClosureCodePopulated>false</CustomerClosureCodePopulated>
        <ManagerClosureCodePopulated>false</ManagerClosureCodePopulated>
        <IncludedClosureCodes />
        <PivotalClosureReason>Closed - Field Declined</PivotalClosureReason>
      </Pivotal_Closure>
      <Pivotal_Closure>
        <!-- NOTES - Voided in Prospects App when failed signatures -->
        <Application_Status_Code>VR</Application_Status_Code>
        <CustomerClosureCodePopulated>false</CustomerClosureCodePopulated>
        <ManagerClosureCodePopulated>false</ManagerClosureCodePopulated>
        <IncludedClosureCodes />
        <PivotalClosureReason>Closed - Field Declined</PivotalClosureReason>
      </Pivotal_Closure>
      <Pivotal_Closure>
        <!-- NOTES - Voided in Lending App when no products available -->
        <Application_Status_Code>VR</Application_Status_Code>
        <CustomerClosureCodePopulated>true</CustomerClosureCodePopulated>
        <ManagerClosureCodePopulated>false</ManagerClosureCodePopulated>
        <IncludedClosureCodes>FA</IncludedClosureCodes>
        <PivotalClosureReason>Closed - Field Declined</PivotalClosureReason>
      </Pivotal_Closure>
      <Pivotal_Closure>
        <!-- NOTES - Voided in Lending App when failed signatures -->
        <Application_Status_Code>VR</Application_Status_Code>
        <CustomerClosureCodePopulated>true</CustomerClosureCodePopulated>
        <ManagerClosureCodePopulated>false</ManagerClosureCodePopulated>
        <IncludedClosureCodes>FS</IncludedClosureCodes>
        <PivotalClosureReason>Closed - Field Declined</PivotalClosureReason>
      </Pivotal_Closure>
      <Pivotal_Closure>
        <!-- NOTES - Agreements Created / Loan Application Completed -->
        <Application_Status_Code>LC</Application_Status_Code>
        <CustomerClosureCodePopulated>false</CustomerClosureCodePopulated>
        <ManagerClosureCodePopulated>false</ManagerClosureCodePopulated>
        <IncludedClosureCodes />
        <PivotalClosureReason>Closed - Issued</PivotalClosureReason>
      </Pivotal_Closure>
    </Pivotal_Closure_Mappings>
  </PivotalClosureConfiguration>
